#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules/vue-tsc/bin/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules/vue-tsc/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules/vue-tsc/bin/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules/vue-tsc/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/registry.npmmirror.com+vue-tsc@0.40.13_typescript@4.9.5/node_modules:/mnt/f/毕业设计/vue3-ts-vite-vant-volar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../bin/vue-tsc.js" "$@"
fi
